{
    "name": "h5-framework-ng",
    "version": "3.0.0",
    "description": "lechebang admin panel front-end framework",
    "engines": {
        "node": ">= 10.13.0"
    },
    "license": "MIT",
    "scripts": {
        "precommit": "npm run lint-staged",
        "ng": "ng",
        "start": "ng serve -o",
        "hmr": "ng serve -c=hmr",
        "build": "ng build --prod --build-optimizer",
        "analyze": "ng build --prod --build-optimizer --stats-json",
        "lint": "npm run lint:ts && npm run lint:style",
        "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
        "lint:style": "stylelint \"{framework}/**/*.less\" --syntax less",
        "lint-staged": "lint-staged",
        "tslint-check": "tslint-config-prettier-check ./tslint.json",
        "e2e": "ng e2e",
        "test": "ng test --watch",
        "test-coverage": "ng test --code-coverage --watch=false",
        "bundle-report": "webpack-bundle-analyzer dist/stats.json",
        "lint:fix": "tslint --project tsconfig.json --fix"
    },
    "dependencies": {
        "@angular/animations": "~11.2.14",
        "@angular/common": "~11.2.14",
        "@angular/compiler": "~11.2.14",
        "@angular/core": "~11.2.14",
        "@angular/forms": "~11.2.14",
        "@angular/platform-browser": "~11.2.14",
        "@angular/platform-browser-dynamic": "~11.2.14",
        "@angular/router": "~11.2.14",
        "@ngx-translate/core": "^13.0.0",
        "@ngx-translate/http-loader": "^6.0.0",
        "classlist.js": "^1.1.20150312",
        "core-js": "^2.5.4",
        "cropperjs": "~1.4.3",
        "deep-extend": "0.5.1",
        "echarts": "^4.2.1",
        "file-saver": "1.3.3",
        "html2canvas": "1.0.0-alpha.12",
        "js-sha1": "0.6.0",
        "lodash": "^4.17.15",
        "moment": "2.22.1",
        "ng-zorro-antd": "^11.4.2",
        "ngx-build-plus": "^11.0.0",
        "ngx-color": "^6.2.0",
        "ngx-countdown": "^11.0.3",
        "qiniu-js": "^2.5.1",
        "quill": "^1.3.6",
        "rxjs": "~6.5.4",
        "rxjs-compat": "^6.5.4",
        "sa-sdk-javascript": "~1.11.8",
        "screenfull": "3.3.1",
        "simple-line-icons": "2.4.1",
        "sortablejs": "^1.10.2",
        "tslib": "^2.0.0",
        "web-animations-js": "^2.3.2",
        "xlsx": "~0.13.1",
        "zone.js": "~0.11.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1102.14",
        "@angular/cli": "~11.2.14",
        "@angular/compiler-cli": "~11.2.14",
        "@angular/language-service": "~11.2.14",
        "@angularclass/hmr": "^3.0.0",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/jszip": "^3.1.2",
        "@types/mockjs": "^1.0.0",
        "@types/node": "^12.11.1",
        "@types/z-schema": "^3.16.32",
        "adm-zip": "^0.4.13",
        "codecov": "^3.0.0",
        "codelyzer": "^6.0.0",
        "gulp": "^4.0.2",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.1.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "karma-remap-istanbul": "^0.6.0",
        "karma-sauce-launcher": "^1.2.0",
        "less": "^2.7.0",
        "less-loader": "^4.0.0",
        "lint-staged": "^5.0.0",
        "mockjs": "^1.0.1-beta3",
        "nz-tslint-rules": "^0.901.2",
        "protractor": "~7.0.0",
        "stylelint": "^12.0.1",
        "stylelint-config-standard": "^17.0.0",
        "through2": "^2.0.3",
        "ts-node": "~7.0.0",
        "tslint": "~6.1.0",
        "typescript": "4.1.6",
        "webpack-bundle-analyzer": "^3.6.0"
    },
    "lint-staged": {
        "framework/**/*.ts": "lint:ts",
        "framework/**/*.less": "lint:style"
    }
}
